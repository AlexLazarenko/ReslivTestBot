<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="by.resliv.bot.pojo">

    <class name="City" table="city_data">
        <id name="cityId" type="long" column="city_id" >
            <generator class="assigned" />
        </id>

        <property name="name" >
            <column name="name" />
        </property>

        <property name="text" >
            <column name="text" />
        </property>
    </class>

</hibernate-mapping>